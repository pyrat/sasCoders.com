<h1>Jobs Matching Your Search</h1>
<br>
<table>
<tr><td><img src=<%=@img %> ></td><td>Search details:<br>
<p>Full Address: <%= @geo.full_address %></p>
<p>Precision: <%= @geo.precision %> </p>
</td></tr></table>
<hr>

<% for job in @jobs %>
  <div>
    <h3>Title: <%= h job.title %></h3> Distance: <%= job.distance %>
    <p>Short description: <%= h job.short_description %> </p>
  </div>
<% end %>